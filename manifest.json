{
  "name": "Blob File Downloader",
  "description": "You can download files from any website.",
  "manifest_version": 3,
  "version": "1.0",
  "permissions": [
    "downloads",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}